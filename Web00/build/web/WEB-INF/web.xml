<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <description>ahihi</description>
        <param-name>cv1</param-name>
        <param-value>Captain</param-value>
    </context-param>
    <context-param>
        <description>ahihihihihi</description>
        <param-name>cv1</param-name>
        <param-value>Thor</param-value>
    </context-param>
    <servlet>
        <servlet-name>ShowServlet</servlet-name>
        <servlet-class>sample.servlet.ShowServlet</servlet-class>
        <init-param>
            <param-name>sc-cv-00</param-name>
            <param-value>Doraemon</param-value>
        </init-param>
        <init-param>
            <param-name>sc-cv-00</param-name>
            <param-value>Nobita</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ProcessServlet</servlet-name>
        <servlet-class>sample.servlet.ProcessServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MidServlet</servlet-name>
        <servlet-class>sample.servlet.MidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndServlet</servlet-name>
        <servlet-class>sample.servlet.EndServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowServlet</servlet-name>
        <url-pattern>/ShowServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessServlet</servlet-name>
        <url-pattern>/ProcessServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MidServlet</servlet-name>
        <url-pattern>/MidServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndServlet</servlet-name>
        <url-pattern>/EndServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
